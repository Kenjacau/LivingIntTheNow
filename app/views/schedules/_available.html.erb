<%= form_for(schedule) do |f| %>
  <% if schedule.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(schedule.errors.count, "error") %> prohibited this schedule from being saved:</h2>

      <ul>
      <% schedule.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

 <div class="field">
    <%# f.label :id %>
    <%= f.hidden_field :id %>
  </div>

  <div class="field">
    <%# f.label schedule.inspect %>
    <%= f.hidden_field :user_id %>
  </div>

  <div class="field">
    <%# f.label :position_id %>
    <%= f.hidden_field :position_id %>
  </div>

  <div class="field">
    <%= f.label :Release %>
    <%= f.check_box:available %>
  </div>

  <div class="field">
    <%# f.label :date %>
    <%= f.date_select :date, { :discard_day => true, :discard_month => true, :discard_year => true } %>
  </div>

  <div class="field">
    <%= f.label :startTime %>
    <%= f.time_select :startTime, {disabled: 'restricted'} %>
  </div>

  <div class="field">
    <%= f.label :endTime %>
    <%= f.time_select :endTime, {disabled: 'restricted'} %>
  </div>

  
  

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
